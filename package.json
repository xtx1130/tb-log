{
  "name": "tb-log",
  "version": "2.0.0",
  "description": "colorful log for info/success/error/start/warn",
  "main": "index.js",
  "scripts": {
    "test": "jest --forceExit",
    "lint": "standard . --fix",
    "cov": "npm run test -- --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "benchmark": "cd benchmark && node benchmarkAsync && node benchmarkSync"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.baidu.com/tbCli/tb-log"
  },
  "keywords": [
    "tb",
    "log"
  ],
  "author": "xietianxin",
  "license": "MIT",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "coveralls": "^3.0.0",
    "jest": "^23.5.0",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "figures": "^2.0.0"
  },
  "jest": {
    "testMatch": [
      "**/test/*.js"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!**/test/**",
      "!**/benchmark/**",
      "**/*.js",
      "!**/coverage/**"
    ]
  }
}
